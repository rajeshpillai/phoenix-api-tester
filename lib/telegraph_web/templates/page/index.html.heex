<h4>DASHBOARD</h4>
  <div class="row">
    <%= for item <- @items do %>
          <div class="column card">
            <h4>
              <a href={Routes.test_path(@conn, :show, item.id)}>
                <%= item.title %>
              </a>
            </h4>
            <div><%= item.description %></div>
          </div>
    <% end %>
  </div>